{
  "name": "wave-ui",
  "version": "2.39.0",
  "description": "An emerging UI framework for Vue.js (2 & 3) with only the bright side. :sunny:",
  "author": "Antoni Andre <antoniandre.web@gmail.com>",
  "main": "./dist/wave-ui.umd.js",
  "unpkg": "dist/wave-ui.umd.js",
  "jsdelivr": "dist/wave-ui.umd.js",
  "module": "./dist/wave-ui.es.js",
  "exports": {
    "./dist/wave-ui.css": "./dist/wave-ui.css",
    "./dist/wave-ui.cjs.js": "./dist/wave-ui.cjs.js",
    "./src/wave-ui": "./src/wave-ui/index.js",
    ".": {
      "import": "./dist/wave-ui.es.js",
      "require": "./dist/wave-ui.umd.js"
    }
  },
  "files": [
    "dist",
    "src/wave-ui"
  ],
  "keywords": [
    "wave ui",
    "ui framework",
    "ui library",
    "vue ui",
    "vue3",
    "vue framework",
    "ui"
  ],
  "license": "MIT",
  "repository": "https://github.com/antoniandre/wave-ui",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base /wave-ui/",
    "build-bundle": "BUNDLE=true vite build && mv ./dist/style.css ./dist/wave-ui.css",
    "serve": "vite preview --root docs --base /wave-ui/",
    "lint": "vite lint"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.vue"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@mdi/font": "^5.9.55",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "3.1.5",
    "autoprefixer": "^10.4.4",
    "axios": "^0.25.0",
    "eslint": "^7.32.0",
    "font-awesome": "^4.7.0",
    "gsap": "^3.10.3",
    "ionicons": "^4.6.3",
    "material-design-icons": "^3.0.1",
    "postcss": "^8.4.12",
    "rollup-plugin-delete": "^2.0.0",
    "sass": "^1.50.0",
    "simple-syntax-highlighter": "^2.2.0",
    "splitpanes": "^3.1.1",
    "vite": "^2.9.5",
    "vite-plugin-pug": "^0.3.1",
    "vue": "^3.2.33",
    "vue-cal": "^4.2.0",
    "vue-router": "^4.0.14",
    "vueperslides": "^3.3.2",
    "vuex": "^4.0.2"
  }
}
